package com.aurionpro.isp.test;

import com.aurionpro.isp.model.IWorkable;
import com.aurionpro.isp.model.IWorker;

public class Worker {
	public static void main(String[] args) {
		IWorker labour = new Labour();
		labour.startWork();
		labour.endWork();
		labour.eat();
		labour.drink();
		
		IWorker robot = new Robot();
		robot.startWork();
		robot.endWork();
		robot.eat();
		robot.drink();
	}
}
