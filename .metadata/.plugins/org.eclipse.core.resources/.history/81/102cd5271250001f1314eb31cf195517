		this.price = price;
		this.discription = discription;
	}

	@Override
	public String getName() {
		// TODO Auto-generated method stub
		return name;
	}

	@Override
	public Double getPrice() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public String getDiscription() {
		// TODO Auto-generated method stub
		return null;
	}

}
